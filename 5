#include <cstdlib>
#include <iostream>
#include <cmath>
 
using namespace std;
 
int main() {
    int i;
    float y, y1, x;
    cout << "Ведите x: ";
    cin >> x;
    y=x;
    do {
        y1=y;
        y=0.5*(y+3*x/(2*y*y + x/y));
    }
    while (abs(y-y1)>1/100000);
    cout <<y<<endl;
    return 0;
}
